{
  "manifest_version": 2,

  "name": "Unlike!",
  "description": "This extension turns all likes to unlikes.",
  "version": "1.0",

  "content_scripts": [
      {
          "matches": ["http://*/*", "https://*/*"],
          "js": [
            "jquery.js",
            "content.js"
          ],
          "all_frames": true
      }
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}